programa
{
	funcao inicio()
	{
		inteiro numeroDigitado
		inteiro numeroQtd = 0
		inteiro menorNumero
	
		enquanto(numeroQtd < 10)
		{
			leia(numeroDigitado)
			se(numeroQtd == 0)
				menorNumero = numeroDigitado
			
			se(numeroDigitado < menorNumero)
				menorNumero = numeroDigitado
			numeroQtd++
		}

		escreva("O menor número é: ",menorNumero)
	}
}
